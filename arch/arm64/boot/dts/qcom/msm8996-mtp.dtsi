&pm8994_mpps {
	pinctrl-names = "default";
	pinctrl-0 = <&pm_mpp2_def &pm_mpp4_def>;

	/* MPP 2 */
	pm8994_mpp2 {
		pm_mpp2_def: gpio {
			pins = "mpp2";
			function = "digital";
			output-low;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* MPP 4 - HDMI_5V_VREG regulator enable*/
	pm8994_mpp4 {
		pm_mpp4_def: gpio {
			pins = "mpp4";
			function = "digital";
			output-high;
			power-source = <PM8994_VPP_1P8>;
		};
	};
};

&pmi8994_gpios {
	pinctrl-names = "default";
	pinctrl-0 = <&pmi8994_gpio_1 &pmi8994_gpio_2 &pmi8994_gpio_3
		     &pmi8994_gpio_4 &pmi8994_gpio_5 &pmi8994_gpio_6
		     &pmi8994_gpio_7 &pmi8994_gpio_8 &pmi8994_gpio_9
		     &pmi8994_gpio_10>;

	pmi8994_gp1 {
		pmi8994_gpio_1: gpio1 {
			pins = "gpio1";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 2  SPKR_SD_N */
	spkr_sd_n {
		pmi8994_gpio_2: speaker_sd_n {
			pins = "gpio2";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 3  SPKR_SD_N */
	spkr_sd2_n {
		pmi8994_gpio_3: speaker_sd2_n {
			pins = "gpio3";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pmi8994_gp4 {
		pmi8994_gpio_4: gpio4 {
			pins = "gpio4";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 5 - USB3 OTG SWITCH EN */
	usb3_otg_switch_en {
		pmi8994_gpio_5: otg_switch_enable {
			pins = "gpio5";
			function = "normal";
			output-low;
			drive-push-pull;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pmi8994_gp6 {
		pmi8994_gpio_6: gpio6 {
			pins = "gpio6";
			function = "normal";
			drive-push-pull;
		};
	};

	pmi8994_gp7 {
		pmi8994_gpio_7: gpio7 {
			pins = "gpio7";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 8 - LCD REG EN, 5V BOOST */
	lcd_reg_en_boost {
		pmi8994_gpio_8: lcd_5v_boost_reg {
			pins = "gpio8";
			function = "normal";
			output-high;
			drive-push-pull;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pmi8994_gp9 {
		pmi8994_gpio_9: gpio9 {
			pins = "gpio9";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 10 - HPH_EN1 */
	hph_en0 {
		pmi8994_gpio_10: hph0_enable {
			pins = "gpio10";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-disable;
			//qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};
};

