		qcom,us-euro-gpios = <&pm8994_mpps 2 0>;
		qcom,wsa-max-devs = <2>;
		qcom,wsa-devs = <&wsa881x_211>, <&wsa881x_212>,
				<&wsa881x_213>, <&wsa881x_214>;
		qcom,wsa-aux-dev-prefix = "SpkrLeft", "SpkrRight",
					  "SpkrLeft", "SpkrRight";
	};
};

&pm8994_gpios {
	pinctrl-names = "default";
	pinctrl-0 = <&pm8994_gpio_1 &pm8994_gpio_2 &pm8994_gpio_3
		     &pm8994_gpio_4 &pm8994_gpio_5 &pm8994_gpio_6
		     &pm8994_gpio_7 &pm8994_gpio_8 &pm8994_gpio_9
		     &pm8994_gpio_10 &pm8994_gpio_11 &pm8994_gpio_11
		     &pm8994_gpio_13 &pm8994_gpio_14 &pm8994_gpio_15
		     &pm8994_gpio_16 &pm8994_gpio_17 &pm8994_gpio_18
		     &pm8994_gpio_19>;

	pm8994_gp1 {
		pm8994_gpio_1: gpio1 {
			pins = "gpio1";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp2 {
		pm8994_gpio_2: gpio2 {
			pins = "gpio2";
			function = "normal";
			//input-enable;
			drive-push-pull;
			bias-high-impedance;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	pm8994_gp3 {
		pm8994_gpio_3: gpio3 {
			pins = "gpio3";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp4 {
		pm8994_gpio_4: gpio4 {
			pins = "gpio4";
			function = "normal";
			//input-enable;
			drive-push-pull;
			bias-high-impedance;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	pm8994_gp5 {
		pm8994_gpio_5: gpio5 {
			pins = "gpio5";
			function = "normal";
			//input-enable;
			drive-push-pull;
			bias-high-impedance;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	pm8994_gp6 {
		pm8994_gpio_6: gpio6 {
			pins = "gpio6";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 7 - NFC DWL REQ */
	nfc_dwl_req {
		pm8994_gpio_7: nfc_download {
			pins = "gpio7";
			function = "normal";
			output-low;
			drive-push-pull;
			//qcom,pull-up-strength = <PMIC_GPIO_PULL_UP_NO>;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 8 - WLAN_EN - CMOS GPIO Output, PD10, SRC VIN2 */
	wlan_en {
		pm8994_gpio_8: wlan_enable {
			pins = "gpio8";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-pull-down;
			//needed? qcom,pull-up-strength = <PMIC_GPIO_PULL_DN_10>;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 9 - Rome 3.3V control - CMOS GPIO Output HIGH, SRC VPH_PWR */
	lan_en {
		pm8994_gpio_9: wwlan_enable {
			pins = "gpio9";
			function = "normal";
			output-high;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_HIGH>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	/* GPIO 10 - NFC CLK _REQ */
	nfc_clk_req {
		pm8994_gpio_10: nfc_clock {
			pins = "gpio10";
			function = "normal";
			input-enable;
			drive-push-pull;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp11 {
		pm8994_gpio_11: gpio11 {
			pins = "gpio11";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp12 {
		pm8994_gpio_12: gpio12 {
			pins = "gpio12";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 13 - HPH_EN0 - CMOS GPIO Output HIGH, SRC1.8V */
	hph_en0 {
		pm8994_gpio_13: hph0_enable {
			pins = "gpio13";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_HIGH>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 14 - lcd_bklt_reg_en - CMOS GPIO Output HIGH, SRC1.8V */
	lcd_bklt_reg_en {
		pm8994_gpio_14: lcd_backlight {
			pins = "gpio14";
			function = "normal";
			output-high;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_HIGH>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp15 {
		pm8994_gpio_15: gpio15 {
			pins = "gpio15";
			function = "func1";
			output-low;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_HIGH>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp16 {
		pm8994_gpio_16: gpio16 {
			pins = "gpio16";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp17 {
		pm8994_gpio_17: gpio17 {
			pins = "gpio17";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 18 - Rome Sleep Clock - CMOS GPIO Output LOW, F2, SRC1.8V */
	rome_sleep_clk {
		pm8994_gpio_18: rome_sleep {
			pins = "gpio18";
			function = "func2";
			output-low;
			drive-push-pull;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_MED>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 19 - Rome BT Reset - CMOS GPIO Output LOW, F2, SRC1.8V */
	pm8994_gp19 {
		pm8994_gpio_19: gpio19 {
			pins = "gpio19";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp22 {
		pm8994_gpio_22: gpio22 {
			pins = "gpio17";
			function = "normal";
			drive-push-pull;
		};
	};
};

